<%= form_for @user do |f| %>
    Name: <%= f.text_field :name %>
    Password: <%= f.password_field :password %>
    Password Confirmation: <%= f.password_field :password_confirmation %>
    Nausea Level: <%= f.text_field :nausea %>
    Happiness Level: <%= f.text_field :happiness %>
    Tickets: <%= f.text_field :tickets %>
    Height in Inches: <%= f.text_field :height %>

    <%= f.submit "Create User" %>
  <% end %>